#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../.2.8.2@webpack-bundle-analyzer/lib/bin/analyzer.js" "$@"
  ret=$?
else 
  node  "$basedir/../.2.8.2@webpack-bundle-analyzer/lib/bin/analyzer.js" "$@"
  ret=$?
fi
exit $ret
